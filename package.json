{
    "name": "vue-package-collection",
    "version": "0.0.0",
    "author": "chrofire",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/chrofire/vue-package-collection"
    },
    "type": "module",
    "scripts": {
        "docs:dev": "vitepress dev docs --host",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs",
        "release": "bumpp --execute=\"npm run release:pre\" --all --no-push",
        "release:version": "bumpp --all --no-push",
        "release:pre": "npm run changelog && git add CHANGELOG.md",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
    },
    "dependencies": {
        "@vueuse/core": "^10.9.0",
        "element-plus": "^2.6.2",
        "lodash-es": "^4.17.21",
        "vue": "^3.4.21"
    },
    "devDependencies": {
        "@iconify-json/ep": "^1.1.15",
        "@stylistic/stylelint-config": "^1.0.1",
        "@stylistic/stylelint-plugin": "^2.1.0",
        "@types/lodash-es": "^4.17.12",
        "@vitejs/plugin-vue-jsx": "^3.1.0",
        "bumpp": "^9.4.0",
        "conventional-changelog-cli": "^4.1.0",
        "czg": "^1.9.0",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.24.0",
        "postcss-html": "^1.6.0",
        "postcss-scss": "^4.0.9",
        "sass": "^1.72.0",
        "stylelint": "^16.3.1",
        "unocss": "^0.58.8",
        "unplugin-auto-import": "^0.17.5",
        "unplugin-vue-components": "^0.26.0",
        "vite": "^5.2.6",
        "vite-plugin-inspect": "^0.8.3",
        "vitepress": "^1.0.1"
    }
}
